<%- include('../../partials/navbar.ejs') %> 

<div class="container">
    <div class="card mt-5">
        <div class="card-header">
            <h2>Criação de usuário</h2>
        </div>
        <div class="card-body">
            <form action="/users/create" method="POST">
                <input class="form-control" type="email" type="email" name="email" placeholder="email@exemplo.com" autocomplete="off">
                <br>
                <input class="form-control" type="password" type="password" name="password" placeholder="" autocomplete="off">
                <br>
                <button type="submit" class="btn btn-success btn-block">Criar</button>
            </form>

        </div>
    </div>
</div>

